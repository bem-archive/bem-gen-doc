block example {

    default, !this.ctx.url: null

    js: true

    content: [
        {
            elem: 'title',
            content: this.ctx.title
        },
        {
            elem: 'live',
            url: this.ctx.url
        }
    ]

    elem live {

        tag: 'iframe'

        attrs: {
            src: this.environ['site-root'] + '/examples/' + this.ctx.url
        }

    }

}