block page {

    elem assets {

        bem: false

        tag: ''

    }

    elem css {

        bem: false

        tag: 'style'

        this.ctx.url {

            tag: 'link'
            attrs: { rel: 'stylesheet', href: this.ctx.url }

        }
    }

    elem js {

        bem: false

        tag: 'script'

        this.ctx.url, attrs: { src: this.ctx.url }

    }

}
